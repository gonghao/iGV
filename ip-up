#!/bin/bash
export PATH=/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:/usr/X11/bin:/usr/local/sbin

#get gw
	#ovpn
    VPNGW=`netstat -nr | grep ^0/1 | grep -P 'tun|tap' | head -1 | awk '{print $2}'`
    echo OVPN:${VPNGW}

	#noneovpn
    if [ -z ${VPNGW} ]; then
		VPNGW=`netstat -nr | grep ^default | grep ppp | head -1 | awk '{print $2}'`
		echo VPN:${VPNGW}
    fi

	#get oldgw	
    OLDGW=`netstat -nr | grep -P '^default|^0/1' | grep -v $(echo ${VPNGW}) | head -1 | awk '{print $2}'`
    echo OLD:${OLDGW}

#log		
	echo "${OLDGW}" > /tmp/vpn_oldgw
	echo "${VPNGW}" > /tmp/vpn_vpngw

dscacheutil -flushcache

#ipup

#dns 202.45.84.58
route add -host 202.45.84.58 "${VPNGW}"
#dnsend

#gfw
